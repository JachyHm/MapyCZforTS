<Window x:Class="MapyCZforTS_CS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapyCZforTS_CS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Menu Grid.Row="0">
            <MenuItem Header="Soubor">
                <MenuItem Header="Konce" />
            </MenuItem>
            <MenuItem Header="Nastavení">
                <MenuItem Header="Pokjročilý logging" IsCheckable="True"/>
                <MenuItem Header="Změna pracovního adrtesáře" />
            </MenuItem>
            <MenuItem Header="O aplikaci..."/>
        </Menu>
    </Grid>
</Window>
